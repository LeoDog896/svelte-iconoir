{
	"name": "@indaco/svelte-iconoir",
	"version": "1.0.0",
	"description": "Iconoir SVG icons to Svelte and SvelteKit.",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"svelte": "src/index.js",
	"files": [
		"dist",
		"iconoir",
		"src",
		"*.svelte"
	],
	"scripts": {
		"generate:icons": "node scripts/buildIconsDataset.mjs",
		"build": "rollup -c",
		"package": "npm run generate:icons",
		"clean": "node scripts/cleanAll.mjs",
		"lint": "prettier --check --plugin-search-dir=. . && eslint",
		"format": "prettier --write --plugin-search-dir=. .",
		"prettier:check": "prettier --check --plugin-search-dir=. ."
	},
	"keywords": [
		"svelte",
		"sveltekit",
		"iconoir",
		"icons",
		"icons-set",
		"svg-icons"
	],
	"author": "indaco <github@mircoveltri.me>",
	"license": "MIT",
	"devDependencies": {
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^13.0.5",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"rollup": "^2.58.0",
		"rollup-plugin-svelte": "^7.1.0",
		"rollup-plugin-terser": "^7.0.2",
		"svelte": "^3.43.1",
		"svg-parser": "^2.0.4"
	},
	"dependencies": {
		"iconoir": "^1.0.0"
	},
	"exports": {
		"./package.json": "./package.json",
		"./src/Icon.svelte": "./src/Icon.svelte",
		"./src/index.js": "./src/index.js",
		".": "./dist/index.js"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/indaco/svelte-iconoir"
	},
	"bugs": {
		"url": "https://github.com/indaco/svelte-iconoir/issues"
	}
}
